jQuery(document).ready(function($) {
function syn_stick_video(div_top) {
    var window_top = $(window).scrollTop();
    
    var vid = $('.syn-video');
    var vid_width = vid.outerWidth();
    var vid_height = vid.outerHeight();
    vid.css('transition', 'transform 0.3s');
    if (window_top > div_top+100) {
        if ($('.single').find('#syn-placeholder').length === 0){
            vid.css({'position' : 'fixed', 'width' : '360px', 'bottom' : '0px', 'right' : '50px', 'z-index' : '1000'});
            vid.after('<div id="syn-placeholder" style="width:'+vid_width+'px;height:'+vid_height+'px;background:lightgray;"></div>');
        }
    } else {
        $('.single').find('#syn-placeholder').remove();
        vid.css({'position' : 'static', 'width' : '100%', 'bottom' : '0px', 'right' : '0px'});
    }
}

$(function() {
    var isSingle = $('body').hasClass('single');
    if ($('.syn-video').length === 0){ return; }
    if(isSingle && $(window).width() > 767){
        var div_top = $('.syn-video').offset().top;
        $(window).scroll(function(){syn_stick_video(div_top);});
        syn_stick_video(div_top);
    }
	
	$(window).resize(function(){
		if(isSingle && $(window).width() < 768){
			$(window).unbind('scroll');
		} else if(isSingle && $(window).width() > 767){
			var div_top = $('.syn-video').offset().top;
			$(window).scroll(function(){syn_stick_video(div_top);});
			syn_stick_video(div_top);
		}
	});
});
});

